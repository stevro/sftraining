<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html" charset="UTF-8" />
        <title>{% block title %}Todo Application!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" media="screen" type="text/css" href="{{asset('bundles/sensiotodo/css/style.css')}}"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="header">
            
        </div>
        {% block body %}
           
            {%block stefan%}{%endblock stefan%}
            
        {% endblock body %}
        
        <div id="footer">
            {{ include('::menu.html.twig', {'menu_id':'menu_bot'})}}
                {{ render(controller('SensioTrainingBundle:Default:counter'))}}
        </div>
        {% block javascripts %}{% endblock %}
    </body>
</html>
